{
  "name": "codesign-validator",
  "version": "0.0.1",
  "description": "Validating codesign of files",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "ava test/lib/index.test.js --timeout=1m",
    "test:watch": "npm run test -- --watch",
    "lint": "esw src/ test/",
    "lint:watch": "npm run lint -- --watch",
    "cover": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "do-publish": "npm run lint && npm run tsc && npm run cover && npm publish"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "README.md"
  ],
  "directories": {
    "dist": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juturu/codesign-validator.git"
  },
  "keywords": [],
  "author": "Hari Juturu <juturu@live.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^9.6.5",
    "ava": "^0.18.2",
    "babel-register": "^6.24.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "eslint-watch": "^3.0.1",
    "nyc": "^10.1.2",
    "sinon": "^2.1.0",
    "typescript": "^2.8.1"
  },
  "dependencies": {},
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
